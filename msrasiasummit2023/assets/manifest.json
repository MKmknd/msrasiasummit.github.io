---
permalink: "manifest.json"
layout: null
sitemap: false
---
{
  "lang": "en",
  "name": "{{ site.title }}",
  "short_name": "{{ site.short_name | default: site.title | replace: ' ', '' }}",
  "theme_color": "{{ site.manifest.theme_color | default: '#24292e' }}",
  "background_color": "{{ site.manifest.background_color | default: '#ffffff' }}",
  "start_url": "/",
  "display": "standalone"
}
